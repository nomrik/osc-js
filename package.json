  "homepage": "https://github.com/nomrik/osc-message#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-spies-next": "^0.9.3",
    "esdoc": "^1.0.4",
